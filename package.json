    "@koa/cors": "3.1.0",
    "@koa/multer": "3.0.0",
    "@koa/router": "9.3.1",
    "axios": "0.19.2",
    "ext-name": "^5.0.0",
    "koa": "2.13.0",
    "lodash": "4.17.21",
    "md5": "2.3.0",
    "module-alias": "2.2.2",
    "moment": "2.27.0",
    "multer": "^1.4.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/plugin-transform-runtime": "7.15.0",
