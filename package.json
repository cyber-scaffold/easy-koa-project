    "start:docker:test": "cross-env ENV_VALUE=docker_test pm2-runtime index.js",
    "build:docker:test": "docker build -f dockerfile.test -t koa-microservice .",
    "start:docker:prod": "cross-env ENV_VALUE=docker_prod pm2-runtime index.js",
    "build:docker:prod": "docker build -f dockerfile.prod -t koa-microservice ."
